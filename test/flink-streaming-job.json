{
    "apiVersion": "operators.srfrnk.com/v1",
    "kind": "FlinkJob",
    "metadata": {
        "name": "flink-streaming-job"
    },
    "spec": {
        "jobManagerUrl": "flink-jobmanager:8081",
        "jarImage": "flink-test:IMAGE_VERSION",
        "jarPath": "/jars/flink-test.jar",
        "mainClass": "flink.test.AppStreaming",
        "streaming": {
            "replicas": 1
        },
        "props": [
            {
                "key": "envKey1",
                "value": "value1"
            },
            {
                "key": "envKey2",
                "value": "value2"
            }
        ]
    }
}